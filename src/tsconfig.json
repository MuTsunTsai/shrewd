{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es2018",
		"removeComments": true,
		"strict": true,
		"preserveConstEnums": true,
		"experimentalDecorators": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"paths": {
			"Index": [
				"./Index"
			]
		}
	},
	"include": [
		"./**/*.ts"
	]
}
